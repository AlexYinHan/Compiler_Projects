FUNCTION add :
PARAM temp
t4 := temp
t5 := t4 + #0
t2 := *t5
t6 := temp
t7 := t6 + #4
t3 := *t7
t1 := t2 + t3
RETURN t1
FUNCTION main :
DEC op 8
t8 := #1
t9 := &op
t10 := t9 + #0
*t10 := t8
t11 := #2
t12 := &op
t13 := t12 + #4
*t13 := t11
t15 := op
ARG t15
t14 := CALL add
n := t14
t16 := n
WRITE t16
t17 := #0
RETURN t17
